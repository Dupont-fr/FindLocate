{
  "name": "backend-FindLocate",
  "version": "1.0.0",
  "description": "backend responsable de l'application FindLocate",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node index.js",
    "dev": "cross-env NODE_ENV=development node --watch index.js",
    "build:ui": "rm -rf dist && cd ../FindLocate_frontend && npm run build && cp -r dist ../FindLocate",
    "deploy:full": "npm run build:ui && git add . && git commit -m uibuild && git push",
    "test": "cross-env NODE_ENV=test node --test",
    "test:only": "cross-env NODE_ENV=test node --test --test-only",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start:test": "cross-env NODE_ENV=test node --watch index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/theblackmamba08/backend-blog.git"
  },
